{
  "name": "creator-single",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "autoprefixer": "postcss -u --no-map autoprefixer --autoprefixer.browsers '&gt; 5%, ie 10' -r public/app.css",
    "build:all": "npm run build:css && npm run build:js",
    "build:css": "npm run clean-css && npm run autoprefixer",
    "build:js": "npm run uglify",
    "clean-css": "cleancss -o public/app.css src/app.css",
    "server": "http-server ./",
    "start": "parallelshell 'npm run server' 'npm run watch:css' 'npm run watch:js'",
    "uglify": "uglifyjs src/app.js -m -o public/app.js",
    "watch:css": "onchange 'src/app.css' -- npm run build:css",
    "watch:js": "onchange 'src/app.js' -- npm run build:js"
  },
  "author": "Drew Barontini",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "clean-css": "^4.0.9",
    "clean-css-cli": "^4.0.9",
    "http-server": "^0.9.0",
    "onchange": "^3.2.1",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^3.0.0",
    "uglify-js": "^2.8.14"
  },
  "devDependencies": {}
}
